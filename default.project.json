{
  "name": "SpaceshipGame",
  "servePort": 34872,
  "tree": {
    "$className": "DataModel",
    "$ignoreUnknownInstances": true,

    "ReplicatedStorage": {
      "Shared": { "$path": "src/shared" }
    },

    "ServerScriptService": {
      "Server": { "$path": "src/server" }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Client": { "$path": "src/client" }
      }
    }
  }
}
